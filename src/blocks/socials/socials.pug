mixin socials(data)
	if data.changeList
		- var changeList=data.changeList
	else
		-
			var changeList = [
				{
					type: 'icon',
					iconLink: '#',
					iconImg: 'fashion-social-18-18-facebook.png',
					iconWidth: 18,
					iconHeight: 18
				},
				{
					type: 'margirn',
					margirnWidth: 10
				},
				{
					type: 'icon',
					iconLink: '#',
					iconImg: 'fashion-social-18-18-vk.png',
					iconWidth: 18,
					iconHeight: 18
				},
				{
					type: 'margirn',
					margirnWidth: 10
				},
				{
					type: 'icon',
					iconLink: '#',
					iconImg: 'fashion-social-18-18-twitter.png',
					iconWidth: 18,
					iconHeight: 18
				}
			]
	if data.social
		+row({
			col: 1,
			alignTd: 'center',
			bg_body: bg_table,
			body_pading_l: body_pading_l,
			body_pading_r: body_pading_r,
			body_width: body_width,
			rtl: false,
			margin_bottom: mr_darkblue,
			content: {
				name: 'social',
				arg: {
					alignment: 'center',
					items: changeList
				}
			}
		})