mixin log-pass(data)
	table(align="left" border="0" cellpadding="0" cellspacing="0" width="100%" style="max-width: 260px;")
		tr 
			td(align="left" bgcolor="#F9F9F9" style="padding: 10px 20px;")
				table(align="center" border="0" cellpadding="0" cellspacing="0" width="100%" style="border-spacing: 0;")
					tr
						td.text(align="left" width="112" style="padding-rigth: 10px;") Your login:
						td.text.text--bold.text--black(align="left")!=data.login
					tr
						td.text(align="left" width="112" style="padding-rigth: 10px;") Your password:
						td.text.text--bold.text--black(align="left")!=data.pass
